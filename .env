# .env

# Aceite do EULA (obrigatório para o Minecraft rodar)
SERVER_EULA=true

# Nome do servidor (exibido no MOTD)
SERVER_NAME=

# Nome do mundo
SERVER_LEVEL_NAME=

# Versão do Spigot
SERVER_VERSION=

# Porta do servidor Minecraft (25565 é a padrão)
SERVER_PORT=

# Modo de jogo: survival, creative, adventure, spectator
SERVER_GAMEMODE=

# Dificuldade: peaceful, easy, normal, hard
SERVER_DIFFICULTY=

# Ativa whitelist (true/false)
SERVER_WHITELIST_ENABLE=

# Lista de jogadores permitidos na whitelist, separados por vírgula
SERVER_WHITELIST_PLAYERS=

# Número máximo de jogadores
SERVER_MAX_PLAYERS=

# Distância de visualização (em chunks)
SERVER_VIEW_DISTANCE=

# Distância de simulação de entidades (em chunks)
SERVER_SIMULATION_DISTANCE=

# Modo online (verificação de autenticação Mojang)
SERVER_ONLINE=

# Timezone do servidor
SERVER_TIMEZONE=

# Proteção de spawn
SERVER_SPAWN_PROTECTION=
